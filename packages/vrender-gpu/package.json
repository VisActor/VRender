    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "portfinder": "^1.0.28",
    "postcss": "8.4.21",
    "postcss-loader": "^6.2.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0",
    "typescript": "4.9.5",
    "webpack": "^5.69.1",
